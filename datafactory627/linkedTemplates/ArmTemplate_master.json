{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory627"
		},
		"datalake6277_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake6277'"
		},
		"dataworkspace627_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'dataworkspace627'"
		},
		"datalake6277_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake6277.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory627_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalake6277_accountKey": {
						"value": "[parameters('datalake6277_accountKey')]"
					},
					"dataworkspace627_connectionString": {
						"value": "[parameters('dataworkspace627_connectionString')]"
					},
					"datalake6277_properties_typeProperties_url": {
						"value": "[parameters('datalake6277_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}